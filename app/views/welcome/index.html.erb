<% if current_user %>
  <%= link_to 'Log Out', '/logout', method: :delete %>
<% else %>
<%= link_to 'Log In', '/login' %>
<%= button_to 'Create New User', '/register', method: :get, local: true %>
<% end %>
<br>
<br>

<h1 style= "text-align: center">Viewing Party Lite</h1>

<h2 style= "text-align: center" >Existing Users:</h2>

  <% @users.each do |user| %>

  <div id="user-<%= user.id%>">
    <p style= "text-align: center"><%= link_to "#{user.email}'s Dashboard", "/dashboard" %></p>
  </div>

  <% end %>
